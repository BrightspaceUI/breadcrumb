{
  "description": "Polymer-based web component for D2L breadcrumbs and related components",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/breadcrumbs.git"
  },
  "name": "d2l-breadcrumbs",
  "scripts": {
    "lint": "npm run lint:wc && npm run lint:js",
    "lint:js": "eslint . --ext .js,.html test/**/*.js test/**/*.html demo/**/*.js demo/**/*.html",
    "lint:wc": "polymer lint",
    "serve": "polymer analyze > analysis.json && polymer serve",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local",
    "test": "npm run lint && npm run test:polymer:local"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@polymer/iron-icon": "^3.0.0-pre.18",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.19.1",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.5",
    "frau-ci": "^1.33.2",
    "polymer-cli": "^1.9.1",
    "sauce-connect-launcher": "^1.2.4",
    "wct-browser-legacy": "^1.0.1"
  },
  "version": "2.2.0",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "main": "d2l-breadcrumbs.js",
  "dependencies": {
    "@polymer/polymer": "^3.0.0",
    "d2l-link": "BrightspaceUI/link#semver:^5",
    "d2l-polymer-behaviors": "Brightspace/d2l-polymer-behaviors-ui#semver:^2"
  }
}
